{
  "name": "topai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:frontend": "node scripts/build-frontend.js",
    "build:full": "cross-env DEPLOY_MODE=full next build",
    "preview": "node scripts/preview-static.js",
    "deploy:manual": "bash scripts/deploy-gh-pages.sh",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "export": "npm run build:frontend",
    "docker:build": "docker build -t topai .",
    "docker:run": "docker run -p 3000:3000 --env-file .env topai",
    "postinstall": "prisma generate",
    "vercel-build": "rm -rf node_modules/.prisma && prisma generate && prisma db push && next build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.954.0",
    "@aws-sdk/s3-request-presigner": "^3.954.0",
    "@prisma/client": "^6.19.0",
    "@tanstack/react-query": "^5.90.12",
    "@vercel/analytics": "^1.5.0",
    "lucide-react": "^0.344.0",
    "next": "14.2.0",
    "next-intl": "^4.6.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.0",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  }
}
